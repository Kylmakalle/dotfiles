<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.akentev.backup-reminders</string>
    <key>WorkingDirectory</key>
    <string>{{ ansible_env.HOME }}/backup-reminders</string>
    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>-c</string>
        <string>make clean {{ Land }} make {{ Land }} make sync</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Minute</key>
        <integer>0</integer>
        <key>Hour</key>
        <integer>0</integer>
    </dict>
    <key>StandardOutPath</key>
    <string>/tmp/com.akentev.backup-reminders.out</string>
    <key>StandardErrorPath</key>
    <string>/tmp/com.akentev.backup-reminders.err</string>
</dict>
</plist>
